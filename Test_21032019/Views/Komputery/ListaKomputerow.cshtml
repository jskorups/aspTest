@model IEnumerable<Test_21032019.Models.Komputer>

@{
    ViewBag.Title = "ListaKomputerow";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>ListaKomputerow</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Firma)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Dostawca)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Uzytkownik)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Firma)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Dostawca)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Uzytkownik)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.Id}) |
            @Html.ActionLink("Details", "Details", new { id=item.Id }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.Id })
        </td>
    </tr>
}

</table>
